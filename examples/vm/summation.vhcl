set N integer
set i integer
set sum integer

set nl char
mov nl 0000000000001010

mov i 0000000000000000
mov sum 0000000000000000
readint N

do:
	gss guard

	clc:
		cmp i N
		geq:
			mov guard 0000000000000000
		leq:
			mov guard 0000000000000001

	act guard:
		add sum i
		inc i

print sum
print nl

halt