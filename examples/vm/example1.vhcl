set n integer
mov n 1010100110

do:
	gss guard1 guard2

	clc:

		push n
		call even

		cmp ..retbol 0001
		equ:
			mov guard1 0001
			mov guard2 0000
		neq:
			mov guard2 0000
			cmp n 0001
			sgt:
				mov guard2 0001
			mov guard1 0000

	act guard1:
		div n 0010
	act guard2:
		mul n 0011
		inc n

print n
halt