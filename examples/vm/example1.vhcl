set n integer
mov n 1010100110

do:
	set guard1 boolean
	set guard2 boolean

	clc:

		push n
		call even

		cmp .retbol 1
		equ:
			mov guard1 1
			mov guard2 0
		neq:
			mov guard2 0
			cmp n 1
			sgt:
				mov guard2 1
			mov guard1 0

	act guard1:
		div n 2
	act guard2:
		mul n 11
		inc n

print n
halt