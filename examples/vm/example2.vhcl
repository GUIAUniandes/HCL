set x alloc.I
set N alloc.I

lll {
	-mov x 0000
	-mov m N
}

set guard1 alloc.B

cmp m x
neq:
	mov guard1 true

do guard1:
	
	push x
	call f
	cmp RESULT 0000:
	equ:
